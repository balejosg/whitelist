{
    "all": true,
    "include": [
        "lib/**/*.js",
        "routes/**/*.js",
        "server.js"
    ],
    "exclude": [
        "tests/**",
        "node_modules/**",
        "coverage/**",
        "lib/swagger.js",
        "lib/error-tracking.js",
        "lib/errors.js"
    ],
    "reporter": [
        "text",
        "lcov",
        "html"
    ],
    "report-dir": "coverage",
    "check-coverage": true,
    "lines": 45,
    "branches": 15,
    "functions": 20,
    "statements": 45
}